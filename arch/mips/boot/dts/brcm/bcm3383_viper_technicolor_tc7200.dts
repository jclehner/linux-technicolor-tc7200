/dts-v1/;

/include/ "bcm3383_viper.dtsi"

/ {
	compatible = "technicolor,tc7200-viper", "brcm,bcm3383-viper";
	model = "Technicolor TC7200 (Viper)";

	memory@0 {
		device_type = "memory";
		reg = <0x00000000 0x08000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
		stdout-path = &uart0;
	};

};

&uart0 {
	status = "okay";
};

&ohci0 {
	status = "disabled";
};

&ehci0 {
	status = "disabled";
};
